{% extends layout %}
{% block meta %}
<title>PHP Surrey</title>
<meta name="description" content="PHP Surrey: A new community driven PHP usergroup in the heart of Surrey (UK) with monthly meetups featuring a range of speakers">
{% endblock meta %}

{% block content %}
<h2><a id="php-surrey" class="anchor" href="#php-surrey" aria-hidden="true"><span class="octicon octicon-link"></span></a>PHP Surrey</h2>

<p><strong>We are proud to announce our November meetup will be on the Wednesday 4th November 2015!</strong></p>
<p>You can view further details <a href="/events">on our events page</a>.</p>

<script type="application/ld+json">
{
  "@context": "http://schema.org",
    "@type": "Event",
    "name": "PHP Surrey November 2015 Meetup",
    "startDate" : "2015-11-04T19:00",
    "endDate" : "2015-11-04T22:00",
    "url" : "http://phpsurrey.uk/events",
    "location" : {
      "@type" : "Place",
      "sameAs" : "http://guildford-institute.org.uk/",
      "name" : "Guildford Institute",
      "address" : "The Guildford Institute, Ward Street, Guildford, GU1 4LH"
    },
    "offers" : [{
      "@type" : "Offer",
      "name" : "General Registration",
      "category" : "primary",
      "price" : "0.00",
      "priceCurrency" : "GBP",
      "availability" : "OnlineOnly",
      "url" : "http://phpsurrey.uk/events"
    }],
    "performer" : [{
      "@type" : "Person",
      "name" : "Chris Sherry",
      "sameAs" : "https://twitter.com/tweetingsherry"
    },
    {
      "@type" : "Person",
      "name" : "Dave Hulbert",
      "sameAs" : "https://twitter.com/dave1010"
    }]
}
